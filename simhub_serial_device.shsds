      "IsEnabled": true,
      "MaximumFrequency": 0
    },
    {
      "Message": {
        "Interpreter": 1,
        "Expression": "var DL_HANDBRAKE = 4\r\nreturn $prop('Handbrake') || ($prop('GameRawData.ShowLights') & DL_HANDBRAKE) || ($prop('DataCorePlugin.CurrentGame') != 'BeamNg' && $prop('JoystickPlugin.Arduino_Leonardo_RZ') > 512) ? 'U1;' : 'U0;'"
      },
      "IsEnabled": true,
      "MaximumFrequency": 0
    },
    {
      "Message": {
        "Interpreter": 1,
        "Expression": "var DL_ABS = 1024\r\nvar beamng_abs = $prop('GameRawData.ShowLights') & DL_ABS\r\nvar assetto_abs = $prop('ABSLevel') != 0 && $prop('DataCorePlugin.CurrentGame') == 'AssettoCorsa'\r\nreturn beamng_abs || assetto_abs ? 'S1;' : 'S0;'"
      },
      "IsEnabled": true,
      "MaximumFrequency": 0
    },
    {
      "Message": {
        "Interpreter": 1,
        "Expression": "switch($prop('DataCorePlugin.CurrentGame'))\r\n{\r\ncase 'AssettoCorsa':\r\n{\r\nreturn $prop('TCLevel') ? 'T1;Z0;' : 'T0;Z1;'\r\nbreak\r\n}\r\ncase 'BeamNg':\r\nvar DL_TC = 16\r\nreturn ($prop('TCActive') ? 'Z0;' : 'Z1;') + ($prop('GameRawData.ShowLights') & DL_TC ? 'T1;' : 'T0;')\r\nbreak;\r\ndefault:\r\nreturn ($prop('TCActive') ? 'T1;' : 'T0;') + ($prop('TCLevel') ? 'Z0;' : 'Z1;')\r\n}"
      },
      "IsEnabled": true,
      "MaximumFrequency": 0
    },
    {
      "Message": {
        "Expression": "if([TurnIndicatorLeft], 'Q1;', 'Q0;')"
      },
      "IsEnabled": true,
      "MaximumFrequency": 0
    },
    {
      "Message": {
        "Expression": "if([TurnIndicatorRight], 'R1;', 'R0;')"
      },
      "IsEnabled": true,
      "MaximumFrequency": 0
    },
    {
      "Message": {
        "Expression": "if([EngineStarted], 'N0;', 'N1;')"
      },
      "IsEnabled": true,
      "MaximumFrequency": 0
    },
    {
      "Message": {
        "Interpreter": 1,
        "Expression": "var DL_FULLBEAM = 2\r\nreturn $prop('GameRawData.ShowLights') & DL_FULLBEAM || $prop('GameRawData.TruckValues.CurrentValues.LightsValues.BeamHigh') ? 'P1;' : 'P0;'"
      },
      "IsEnabled": true,
      "MaximumFrequency": 0
    },
    {
      "Message": {
        "Expression": "if([GameRawData.TruckValues.CurrentValues.MotorValues.BrakeValues.ParkingBrake], 'J1;','J0;')"
      },
      "IsEnabled": true,
      "MaximumFrequency": 0
    },
    {
      "Message": {
        "Expression": "if([GameRawData.Graphics.IsInPit] or [GameRawData.Graphics.isInPitLane], 'b1;', 'b0;')"
      },
      "IsEnabled": true,
      "MaximumFrequency": 0
    },
    {
      "Message": {
        "Expression": "if([CarDamagesAvg] >= 50,'L1;Y1;','L0;Y0;')"
      },
      "IsEnabled": true,
      "MaximumFrequency": 0
